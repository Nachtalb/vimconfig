{
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnSaveFiletypes": ["python"],
    "coc.preferences.willSaveHandlerTimeout": 2000,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "~/.pyenv/versions/nvim/bin/flake8",
    "python.linting.flake8Args": ["--config", "~/.flake8"],
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "~/.pyenv/versions/nvim/bin/black",
    "python.formatting.autopep8Path": "~/.pyenv/versions/nvim/bin/autopep8",
    "python.formatting.yapfPath": "~/.pyenv/versions/nvim/bin/yapf",
    "python.formatting.yapfArgs": ["--style={ column_limit: 120 }"],
    "python.analysis.extraPaths": [
        "./parts/omelette"
    ],
    "python.linting.ignorePatterns": [
        ".vscode/*.py",
        "**/site-packages/**/*.py",
        "bin/*",
        "var/**/*"
    ],
    "files.exclude": {
        "**/*.h*": true,
        "**/*.py[co]": true,
        "**/*.so": true,
        "**/__pycache__": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/chameleon-cache": true
    },
    "files.associations": {
        "*.zcml": "xml"
    },
    "cSpell.language": "en-GB",
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        "coc-settings.json"
    ],
    "cSpell.ignoreRegExpList": [
        "^(\" )?Plug '.*'",
        "from [\\w\\.]+ import [\\w]+",
        "import [\\w\\.]+( as \\w)?"
    ],
    "cSpell.userWords": [
      "Cybercrime",
      "Datatype",
      "Deserialization",
      "Healthcheck",
      "Kubernetes",
      "LLMNR",
      "Nachtalb",
      "Pixiv",
      "TLDR",
      "TQDM",
      "VMIDs",
      "asyncio",
      "autostarted",
      "bootdisk",
      "bruteforce",
      "centos",
      "chasseral",
      "cloudinit",
      "cryptpad",
      "delfino",
      "edcsa",
      "guaconfig",
      "healthchecks",
      "keylogger",
      "linefeeds",
      "microservices",
      "mkdocs",
      "nameserver",
      "networkmask",
      "opencv",
      "pacman",
      "pfsense",
      "playbook",
      "playbooks",
      "posix",
      "rocketchat",
      "selectattr",
      "shiz",
      "threema",
      "timezone",
      "toggleable",
      "traefik",
      "ugoira",
      "usecase",
      "verca",
      "virtio",
      "webp",
      "whitespaces",
      "wlan"
    ],
    "files.maxMemoryForLargeFilesMB": 12288,
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",
    "languageserver": {
        "terraform": {
            "command": "terraform-ls",
            "args": ["serve"],
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        }
    },
    "suggest.completionItemKindLabels": {
        "variable": "\uf71b",
        "constant": "\uf8ff",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\ufa52",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "color": "\ue22b",
        "property": "\ufab6",
        "field": "\uf93d",
        "unit": "\uf475",
        "file": "\uf471",
        "value": "\uf8a3",
        "event": "\ufacd",
        "folder": "\uf115",
        "keyword": "\uf893",
        "snippet": "\uf64d",
        "operator": "\uf915",
        "reference": "\uf87a",
        "typeParameter": "\uf278",
        "default": "\uf29c"
    },
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "diagnostic-languageserver.mergeConfig": true,
    "diagnostic-languageserver.filetypes": {
      "vim": "vint",
      "sh": "shellcheck",
      "yaml": "yamllint"
    },
    "diagnostic-languageserver.formatFiletypes": {
      "lua": "lua-format",
      "sh": "shfmt"
    }
}
